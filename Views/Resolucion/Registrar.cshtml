@section header{
    <div class="page-header page-header-light">
        <div class="page-header-content header-elements-md-inline">
            <div class="page-title d-flex">
                <h4>
                    <span class="font-weight-semibold"> REGISTRAR RESOLUCIÓN</span>
                </h4>
                <a href="#" class="header-elements-toggle text-default d-md-none"><i class="icon-more"></i></a>
            </div>
            <div class="header-elements d-none">
                <div class="d-flex justify-content-center">
                    <button type="button" class="btn btn-link btn-float text-default" id="btnBuscar">
                        <i class="icon-search4 text-slate-700"></i><span>BUSCAR</span>
                    </button>
                    <button type="button" class="btn btn-link btn-float text-default" id="btnRegistrarResolucion">
                        <i class="icon-floppy-disk text-slate-700"></i><span>REGISTRAR RESOLUCION</span>
                    </button>
                </div>
            </div>
        </div>
        <div class="breadcrumb-line breadcrumb-line-light header-elements-md-inline">
            <div class="d-flex">
                <div class="breadcrumb">
                    <a href="#" class="breadcrumb-item"><i class="icon-home2 mr-2"></i> INICIO</a>
                    <a href="#" class="breadcrumb-item"> REGISTRAR RESOLUCIÓN</a>
                </div>
                <a href="#" class="header-elements-toggle text-default d-md-none"><i class="icon-more"></i></a>
            </div>

        </div>
    </div>
}
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <form id="frmFiltro" autocomplete="off">
                    <div class="row">
                        <div class="col-lg-2">
                            <div class="form-group">
                                <label for=""><b><i class="icon-calendar mr-1"></i>NRO DE EXPEDIENTE:</b></label>
                                <div class="input-group input-group-sm">
                                    <div class="input-group input-group-sm">
                                        <input type="text" class="form-control form-control-sm" id="nroExpediente"
                                               name="nroExpediente">
                                        <span class="input-group-append">
                                            <button class="btn btn-light" type="button">
                                                <i class="icon-user"></i>
                                            </button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2">
                            <div class="form-group">
                                <label for=""><b><i class="icon-calendar mr-1"></i>AÑO DERIVACIÓN:</b></label>
                                <div class="input-group input-group-sm">
                                    <div class="input-group input-group-sm">
                                        <select id="CbAnios" name="anio" class="form-control"
                                                data-container-css-class="select-sm">
                                            @{
                                                for (int i = 2010; i < 2050; i++)
                                                {
                                                    <option value="@i">@i</option>
                                                }
                                            }
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label for=""><b>FECHA DE REGISTRO:</b></label>
                                    <input type="text" class="form-control form-control-sm" readonly
                                           id="fechaExpediente">
                                </div>
                            </div>
                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label for=""><b>NRO DERIVACIÓN:</b></label>
                                    <input type="text" class="form-control form-control-sm" readonly
                                           id="nroDerivacionExpediente">
                                </div>
                            </div>
                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label for=""><b>CÓDIGO ESCALAFÓN:</b></label>
                                    <input type="text" class="form-control form-control-sm" readonly
                                           id="codigoEscalafonExpediente">
                                </div>
                            </div>
                            <div class="col-lg-3">
                                <div class="form-group">
                                    <label for=""><b>NOMBRE Y APELLIDO:</b></label>
                                    <input type="text" class="form-control form-control-sm" readonly
                                           id="nombreApellidoExpediente">
                                </div>
                            </div>
                            <div class="col-lg-3">
                                <div class="form-group">
                                    <label for=""><b>MOTIVO:</b></label>
                                    <input type="text" class="form-control form-control-sm" id="motivoExpediente"
                                           readonly>
                                </div>
                            </div>


                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label for=""><b>ESTABLECIMIENTO:</b></label>
                                    <input type="text" class="form-control form-control-sm" readonly
                                           id="establecimientoExpediente">
                                </div>
                            </div>

                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label for=""><b>OBSERVACIÓN:</b></label>
                                    <input type="text" class="form-control form-control-sm" readonly
                                           id="observacionExpediente">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <fieldset>
                            <legend class="font-weight-semibold text-uppercase font-size-sm">
                                <i class="icon-file-text2 mr-2"></i>
                                DATOS DE RESOLUCIÓN
                            </legend>
                        </fieldset>
                    </div>
                    <div class="col-lg-12">
                        <form id="frmResolucion" autocomplete="off">
                            <div class="row">
                                <div class="col-lg-4">
                                    <label for=""><b><i class="icon-calendar mr-1"></i>NÚMERO DE RESOLUCIÓN:</b></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control form-control-sm text-center"
                                               name="codigo">
                                        <input type="text" class="form-control form-control-sm text-center"
                                               name="codigoAnio" placeholder="@DateTime.Now.Year">
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label for=""><b>FECHA DE ASIGNACIÓN:</b></label>
                                        <input type="text" class="form-control form-control-sm Fecha"
                                               name="fechaAsignacion" id="fechaAsignacion">
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label for=""><b>MOTIVO:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="motivo"
                                               id="motivo">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>DNI:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="dni" id="dni">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>TITULO PROFESIONAL:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="tituloProfesional"
                                               id="tituloProfesional">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>ESPECIALIDAD:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="especialidad"
                                               id="especialidad">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>ESTABLECIMIENTO:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="establecimiento"
                                               id="establecimiento">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>NIVEL MAGISTERIAL:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="nivelMagisterial"
                                               id="nivelMagisterial">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>JORNADA LABORAL:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="jornadaLaboral"
                                               id="jornadaLaboral">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>RÉGIMEN DE PENSIÓN:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="regimenPension"
                                               id="regimenPension">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>Nº IPSS:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="nroIpss"
                                               id="nroIpss">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>FECHA DE INGRESO:</b></label>
                                        <input type="text" class="form-control form-control-sm Fecha"
                                               name="fechaIngreso" id="fechaIngreso">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>FECHA DE CESE:</b></label>
                                        <input type="text" class="form-control form-control-sm Fecha" name="fechaCese"
                                               id="fechaCese">
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    <div class="form-group">
                                        <label for=""><b>CÓDIGO ESCALAFÓN:</b></label>
                                        <input type="text" class="form-control form-control-sm" name="codigoEscalafon"
                                               id="codigoEscalafon">
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section js{
    <script src="~/styles/js/Resolucion/ResolucionRegistrar.js"></script>
}